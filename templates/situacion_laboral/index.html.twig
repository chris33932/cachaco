{% extends 'base.html.twig' %}

{% block title %}SituacionLaboral index{% endblock %}

{% block body %}
<div class="caja">
    <h1>Situacion Laboral</h1>
</div>
 <div class="container">   

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Descripcion</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for situacion_laboral in situacion_laborals %}
            <tr>
                <td>{{ situacion_laboral.id }}</td>
                <td>{{ situacion_laboral.descripcion }}</td>
                <td>
                    <a href="{{ path('situacion_laboral_show', {'id': situacion_laboral.id}) }}">show</a>
                    <a href="{{ path('situacion_laboral_edit', {'id': situacion_laboral.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('situacion_laboral_new') }}">Create new</a>
</div>
{% endblock %}
