{% extends "reportes/baseRepo.html.twig" %}
{% block title %}Reportes femicidios{% endblock %}


{% block reporte %}
<div class="container-fluid">
    <div class="table table-responsive">
    <caption class="text-justify" > <h4>Informe sobre femicidios(contexto femicidio y tipo femicidio )</h4>  
      {{ fecha_desde | fechaCorta }} hasta {{ fecha_hasta | fechaCorta }}</caption>

                             <table class="table table-hover">
            
                                                        <thead style="background-color: #a4eaec;">
                                                            <tr>
                                                                <th>presAutor</th>
                                                                <th>victima</th>
                                                                <th>hecho</th>
                                                                <th>tipoFemicicio</th>
                                                                <th>fecha</th>
                                                                <th>anio</th>
                                                                <th>mes</th>
                                                                <th>hora</th>
                                                                <th>dia</th>
                                                                <th>franjaSeis</th>
                                                                <th>franjaTres</th>
                                                                <th>barrio</th> 
                                                                <th>calle</th>
                                                                <th>altura</th>
                                                                <th>interseccion</th>
                                                                <th>calleInterseccion</th>
                                                                <th>repGeo</th>
                                                                <th>latitud</th>
                                                                <th>longitud</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            {% for fila in datos %}
                                                            <tr>
                                                                <td>{{ fila.presAutor  }}</td>
                                                                <td>{{ fila.victima  }}</td>
                                                                <td>{{ fila.hecho  }}</td>
                                                                <td>{{ fila.tipoFemicicio  }}</td>
                                                                <td>{{ fila.fecha  }}</td>
                                                                <td>{{ fila.anio  }}</td>
                                                                <td>{{ fila.mes  }}</td>
                                                                <td>{{ fila.hora  }}</td>
                                                                <td>{{ fila.dia  }}</td>
                                                                <td>{{ fila.franjaSeis  }}</td>
                                                                <td>{{ fila.franjaTres  }}</td>
                                                                <td>{{ fila.barrio  }}</td>
                                                                <td>{{ fila.calle  }}</td>
                                                                <td>{{ fila.altura  }}</td>
                                                                <td>{{ fila.interseccion  }}</td>
                                                                <td>{{ fila.calleInterseccion  }}</td>
                                                                <td>{{ fila.repGeo  }}</td>
                                                                <td>{{ fila.latitud  }}</td>
                                                                <td>{{ fila.longitud  }}</td>
                                                            
                                                                
                                                            
                                                            </tr>
                                                            {% else %}
                                                            <tr><td>No hay resultados disponibles...</td></tr>
                                                            {% endfor %}
                                                        </tbody>
                                                    </table>
       
      
    </div>
</div>
{% endblock %}
{% block body %}
    {% set path_excel = "" %}
    {{ parent() }}
{% endblock %}

{% block javascripts %}
{{ parent() }}

<script src="{{ asset('js/bootstrap-datepicker.min.js') }}"></script>
<script>
    $(document).ready(function(){
        $('#imprimir').on('click', function(e){
            e.preventDefault();
            window.print();
        });

</script>
{% endblock %}